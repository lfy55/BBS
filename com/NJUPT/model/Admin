package com.njupt.medol;

import java.util.Date;

/**
 * Admin定义了用户类
 * @author lfy
 *
 */
public class Admin {
	private int userId;//用户id
	private String adminName;//用户姓名
	private String adminPass;//用户密码
	private Date createDate;//用户注册日期
	private int adminLimits;//用户权限
	
	public Admin() {}
	
	
	public Admin(int userId, String adminName, String adminPass,
			Date createDate, int adminLimits) {
		super();
		this.userId = userId;
		this.adminName = adminName;
		this.adminPass = adminPass;
		this.createDate = createDate;
		this.adminLimits = adminLimits;
	}


	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getAdminName() {
		return adminName;
	}
	public void setAdminName(String adminName) {
		this.adminName = adminName;
	}
	public String getAdminPass() {
		return adminPass;
	}
	public void setAdminPass(String adminPass) {
		this.adminPass = adminPass;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	public int getAdminLimits() {
		return adminLimits;
	}
	public void setAdminLimits(int adminLimits) {
		this.adminLimits = adminLimits;
	}
	
	public String toString() {
		return "userId:" + userId + " adminName:" + adminName + " adminPass:" + adminPass + " createDate:" +createDate + " adminLimits:" + adminLimits;
	}

}
